class SoundofRangiCore {
    constructor() {
        this.baseFrequency = 432; // Your 432Hz system
        this.lambdaFrequency = 111.11; // Return to Sender
        this.noteColors = {
            'C': '#FF00FF', 'D': '#FFA500', 'E': '#FFFF00', 'F': '#0000FF',
            'G': '#800080', 'A': '#FFFFFF', 'B': '#FF0000'
        };
    }
    
    // Shared frequency calculation
    calculateMarketFrequency(priceChange) {
        return this.baseFrequency * (1 + priceChange / 100);
    }
}
