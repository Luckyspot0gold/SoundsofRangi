# example (run on a mirrored clone)
pip install git-filter-repo
git clone --mirror git@github.com:OWNER/REPO.git
cd REPO.git
# create replacements.txt with lines like: ====BEGIN====\nsecret_value\n====END====\nreplacement_token
git filter-repo --replace-text replacements.txt
git push --force --all
AVALANCHE_API_KEY=your_avalanche_key_here
COINBASE_API_KEY=your_coinbase_key_here
COINBASE_API_SECRET=your_coinbase_secret_here
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
STELLAR_HORIZON_URL=https://horizon.stellar.org
HEDERA_NETWORK=testnet
